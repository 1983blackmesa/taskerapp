{
  "name": "keeper",
  "version": "1.0.0",
  "description": "Tasker - React app that does note keeping, user map location, to do list, login/register with JWT authentication!",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n \"server,client\" -c \"red,green\" \"nodemon server.js\" \"npm run client\""
  },
  "author": "Masis",
  "license": "ISC",
  "engines": {
    "node": "12.18.3"
  },
  "proxy": "http://localhost:5000",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "nodemon": "^2.0.4"
  }
}
